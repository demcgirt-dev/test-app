<h1 style="text-align: center;">Welcome to <%= @user.username %> 's account</h1>

<div class = "row">
  <div class = "col-md-4 col-md-offset-4 center">
    <h3><%= @user.username %>'s articles</h3>
  </div>
  
  <div class = "col-md-10 col-md-offset-1 center">
    <table>
    <tr>
      <th>Title</th>
      <th style = "padding-left: 40px">Description</th>
    </tr>
  
    <%= render 'articles/article', obj: @user.articles %>
    </table>
  </div>
</div>