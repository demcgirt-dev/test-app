<% obj.each do |article| %>
  <tr>
    <td> <%= article.title %> </td>
    <td style = "padding-left: 40px;"> <%= article.description %> </td>
    <td> <%= @entry %></td>
    <td style = "padding-left: 40px;"> <%= link_to 'Edit | ', edit_article_path(article) %></td>
    <td> <%= link_to 'Show | ', article_path(article) %></td>
    <td> <%= link_to 'Delete', article_path(article), method: :delete, data: {confirm: "Are you sure?"} %></td>
  </tr>
<% end %>